public class Main
{
    
    static long fib(long n, long a, long b ) 
    {  
          
        if (n == 0) 
            return a; 
        if (n == 1) 
            return b; 
        return fib(n - 1, b, a + b); 
    } 
    
	public static void main(String[] args) {
	    long n = 80; 
		System.out.println("fib(" + n +") = "+fib(n,0,1) );  
	}
}
